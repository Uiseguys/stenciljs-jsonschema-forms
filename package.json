{
  "name": "@ui-guys/form-generator",
  "private": false,
  "version": "0.0.4",
  "description": "StencilJS implementation of forms generated using JSON Schema",
  "engines": {
    "node": ">=6.11.0 <9.0"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --no-cache",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "prepublish": "stencil build --prod --es5"
  },
  "dependencies": {
    "@stencil/core": "0.7.24",
    "@stencil/dev-server": "latest",
    "@stencil/router": "0.0.28",
    "@stencil/sass": "0.0.2",
    "@stencil/utils": "latest",
    "@types/bootstrap": "^4.0.1",
    "@types/jest": "^21.1.1",
    "ajv": "^6.1.1",
    "bootstrap": "^4.0.0",
    "bootstrap.native": "^2.0.21",
    "http-server": "^0.11.1",
    "jest": "^21.2.1",
    "moment": "^2.20.1",
    "pikaday": "^1.7.0",
    "tslint": "^5.7.0"
  },
  "devDependencies": {
    "@stencil/sass": "0.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uiseguys/stenciljs-jsonschema-forms.git"
  },
  "author": "wiseguys-admin <admin@uiguys.berlin>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Uiseguys/stenciljs-jsonschema-forms/issues"
  },
  "homepage": "https://github.com/Uiseguys/stenciljs-jsonschema-forms",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
